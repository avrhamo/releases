{
  "name": "api-workspace",
  "private": true,
  "version": "0.0.1",
  "description": "The Ultimate Developer Toolkit for REST API Development",
  "author": {
    "name": "Ayele Avraham",
    "email": "ayele.avraham@example.com"
  },
  "main": "main.js",
  "homepage": "https://github.com/avrhamo/api-workspace",
  "repository": {
    "type": "git",
    "url": "https://github.com/avrhamo/api-workspace.git"
  },
  "keywords": [
    "api",
    "testing",
    "mongodb",
    "kafka",
    "developer-tools",
    "rest-api",
    "electron"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:prod": "cross-env NODE_ENV=production tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint .",
    "clean": "rimraf dist/ dist-electron/ node_modules/.vite/",
    "clean:all": "rimraf dist/ dist-electron/ node_modules/",
    "electron:tsc": "tsc -p electron/tsconfig.json",
    "electron:dev": "cross-env NODE_ENV=development npm run electron:tsc && concurrently \"vite\" \"electron .\"",
    "electron:build": "npm run build:prod && npm run electron:tsc && electron-builder",
    "start": "npm run electron:dev",
    "build:mac": "npm run build:prod && npm run electron:tsc && electron-builder --mac",
    "build:win": "npm run build:prod && npm run electron:tsc && electron-builder --win --x64",
    "build:linux": "npm run build:prod && npm run electron:tsc && electron-builder --linux",
    "build:all": "npm run build:prod && npm run electron:tsc && electron-builder --mac --win --linux",
    "build:portable": "npm run build:prod && npm run electron:tsc && electron-builder --win --config.win.target=portable",
    "dist": "npm run build:all",
    "dist:mac": "npm run build:mac",
    "dist:win": "npm run build:win",
    "dist:linux": "npm run build:linux"
  },
  "dependencies": {
    "kafkajs": "^2.2.4",
    "mongodb": "^6.15.0",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@headlessui/react": "^2.2.2",
    "@heroicons/react": "^2.2.0",
    "@monaco-editor/react": "^4.7.0",
    "@tailwindcss/forms": "^0.5.10",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/crypto-js": "^4.2.2",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.14.1",
    "@types/node-fetch": "^2.6.12",
    "@types/openpgp": "^4.4.22",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/recharts": "^1.8.29",
    "@types/swagger-ui-react": "^5.18.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.14",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.2.0",
    "diff": "^5.1.0",
    "electron": "^35.2.1",
    "electron-builder": "^24.1.2",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "js-yaml": "^4.1.1",
    "jsdom": "^26.1.0",
    "jszip": "^3.10.1",
    "jwt-decode": "^4.0.0",
    "openpgp": "^6.1.0",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "recharts": "^3.2.1",
    "rimraf": "^3.0.2",
    "swagger-ui-react": "^5.21.0",
    "tailwindcss": "^3.3.0",
    "terser": "^5.43.1",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.1",
    "vite-plugin-monaco-editor": "^1.1.0",
    "vitest": "^3.2.4"
  }
}